{
  "name": "highnode",
  "version": "1.0.1",
  "description": "Node module to make highcharts a little more usable",
  "license": "MIT",
  "repository": "hjonasson/highnode",
  "author": {
    "name": "Hjortur Jonasson",
    "email": "hjorturjonasson@gmail.com"
  },
  "engines": {
    "node": ">=4"
  },
  "scripts": {
